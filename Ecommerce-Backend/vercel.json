{
    "version": 2,
    "name": "Ecommerce-shop",
    "builds": [
       { "src": "Server.js", "use": "@vercel/node" }
    ],
    "routes": [ 
      { "src": "/_next/static/(.*)", "dest": "/_next/static/$1" },
      { "src": "/static/js/(.*)", "dest": "/static/js/$1" },
      { "src": "/static/css/(.*)", "dest": "/static/css/$1" },
      { "src": "/static/media/(.*)", "dest": "/static/media/$1" },
       { "src": "/(.*)", "dest": "/Server.js" }
    ]
 }