{
   "version": 2,
   "builds": [{ "src": "server.js", "use": "@vercel/node" }],
   "routes": [
     { "src": "/static/js/(.*)", "dest": "/static/js/$1" },
     { "src": "/static/css/(.*)", "dest": "/static/css/$1" },
     { "src": "/static/media/(.*)", "dest": "/static/media/$1" },
     { "src": "/(.*)", "dest": "/server.js" }
   ]
 }
 